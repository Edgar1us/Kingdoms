<odoo>
    <data>
        <!-- explicit list view definition -->
        <record model="ir.ui.view" id="kingdoms.list">
            <field name="name">kingdoms list</field>
            <field name="model">kingdoms.kingdoms</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="population"/>
                    <field name="gold"/>
                    <field name="kings"/>

                </tree>
            </field>
        </record>



        <record model="ir.ui.view" id="kingdoms.kingdoms_form">
            <field name="name">kingdoms list</field>
            <field name="model">kingdoms.kingdoms</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="population"/>
                            <field name="gold"/>
                            <field name="photo" widget="image"/>


                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="kingdoms.kings_tree">
            <field name="name">kings list</field>
            <field name="model">kingdoms.kings</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="race"/>
                    <field name="sex"/>
                    <field name="kingdoms"/>
                    <field name="group_killers"/>

                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="kingdoms.kings_form">
            <field name="name">kings list</field>
            <field name="model">kingdoms.kings</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="race"/>
                            <field name="sex"/>
                            <field name="kingdoms"/>
                            <field name="group_killers"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="kingdoms.dungeons_tree">
            <field name="name">dungeons list</field>
            <field name="model">kingdoms.dungeons</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="killers"/>


                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="kingdoms.dungeons_form">
            <field name="name">dungeons list</field>
            <field name="model">kingdoms.dungeons</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="killers"/>


                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="kingdoms.killers_tree">
            <field name="name">killers list</field>
            <field name="model">kingdoms.killers</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="years"/>
                    <field name="weapon"/>
                    <field name="drug"/>


                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="kingdoms.killers_form">
            <field name="name">killers list</field>
            <field name="model">kingdoms.killers</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="years"/>
                            <field name="weapon"/>
                            <field name="drug"/>



                        </group>
                    </sheet>
                </form>
            </field>
        </record>



        <record model="ir.ui.view" id="kingdoms.heros_tree">
            <field name="name">heros list</field>
            <field name="model">kingdoms.heros</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="years"/>
                    <field name="level"/>
                    <field name="energy"/>
                    <field name="weapon_type"/>
                    <field name="luck"/>


                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="kingdoms.heros_form">
            <field name="name">heros list</field>
            <field name="model">kingdoms.heros</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="years"/>
                            <field name="level"/>
                            <field name="energy"/>
                            <field name="weapon_type"/>
                            <field name="luck"/>



                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- actions opening views on models -->

        <record model="ir.actions.act_window" id="kingdoms.action_window">
            <field name="name">kingdoms window</field>
            <field name="res_model">kingdoms.kingdoms</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="kingdoms.kings_action_window">
            <field name="name">kings window</field>
            <field name="res_model">kingdoms.kings</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="kingdoms.dungeons_action_window">
            <field name="name">dungeons window</field>
            <field name="res_model">kingdoms.dungeons</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="kingdoms.killers_action_window">
            <field name="name">killers window</field>
            <field name="res_model">kingdoms.killers</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.actions.act_window" id="kingdoms.heros_action_window">
            <field name="name">heros window</field>
            <field name="res_model">kingdoms.heros</field>
            <field name="view_mode">tree,form</field>
        </record>







        <!-- server action to the one above -->
        <!--
            <record model="ir.actions.server" id="kingdoms.action_server">
              <field name="name">kingdoms server</field>
              <field name="model_id" ref="model_kingdoms_kingdoms"/>
              <field name="state">code</field>
              <field name="code">
                action = {
                  "type": "ir.actions.act_window",
                  "view_mode": "tree,form",
                  "res_model": model._name,
                }
              </field>
            </record>
        -->

        <!-- Top menu item -->

        <menuitem name="kingdoms" id="kingdoms.menu_root"/>

        <!-- menu categories -->

        <menuitem name="MENU" id="kingdoms.menu_management" parent="kingdoms.menu_root"/>

        <!-- actions -->

        <menuitem name="Kingdoms" id="kingdoms.menu_1_list" parent="kingdoms.menu_management"
                  action="kingdoms.action_window"/>

        <menuitem name="kings" id="kingdoms.menu_2_kings" parent="kingdoms.menu_management"
                  action="kingdoms.kings_action_window"/>

        <menuitem name="dungeons" id="kingdoms.menu_3_dungeons" parent="kingdoms.menu_management"
                  action="kingdoms.dungeons_action_window"/>

        <menuitem name="killers" id="kingdoms.menu_4_killers" parent="kingdoms.menu_management"
                  action="kingdoms.killers_action_window"/>

        <menuitem name="heros" id="kingdoms.menu_5_heros" parent="kingdoms.menu_management"
                  action="kingdoms.heros_action_window"/>


    </data>
</odoo>